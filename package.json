{
  "name": "lottery-grants",
  "version": "0.0.2",
  "description": "Lottery grants explorer tool",
  "scripts": {
    "copy-data-files": "mkdir -p dist/data && cp data/out/* dist/data/",
    "copy-annual-reports-files": "mkdir -p data/annual-reports && cp data/annual-reports/* dist/data/annual-reports",
    "predev": "npm run copy-data-files",
    "prepredev": "npm run copy-annual-reports-files",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "postbuild": "npm run copy-data-files",
    "postpostbuild": "npm run copy-annual-reports-files",
    "webflow-import": "npx import-webflow webflow-export.zip",
    "lint": "eslint src/js/ --ext js",
    "test": "cypress run"
  },
  "importWebflowExport": {
    "copyTrees": {
      "css": "src/css",
      "js": "src/js",
      "images": "src/images"
    },
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "src",
        "transforms": "./src/js/webflow-transform.js"
      }
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^10.7.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "import-webflow-export": "0.0.5",
    "parcel": "^2.7.0"
  },
  "dependencies": {
    "@parcel/transformer-css": "^2.8.2",
    "d3": "^7.6.1",
    "d3-fetch": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "jquery": "^3.6.1",
    "js-worker-search": "github:ranihorev/js-worker-search",
    "lightningcss": "^1.18.0"
  }
}
